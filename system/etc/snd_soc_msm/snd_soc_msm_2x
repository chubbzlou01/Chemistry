SectionDevice
	Name "Handset"
	Comment "Handset Rx device"

	EnableSequence
		'PRI_RX Channels':0:One
		'RX1 MIX1 INP1':0:RX1
		'EAR PA Gain':0:POS_6_DB
		'DAC1 Switch':1:1
		'RX1 Digital Volume':1:69%
	EndSequence

	DisableSequence
		'RX1 MIX1 INP1':0:ZERO
		'DAC1 Switch':1:0
		'EAR PA Gain':0:ZERO
		'RX1 Digital Volume':1:0
	EndSequence

	ACDBID	7:1
	
EndSection

SectionDevice
	Name "Handset Extra Vol"
	Comment "Handset Extra Vol Rx device"

	EnableSequence
		'PRI_RX Channels':0:One
		'RX1 MIX1 INP1':0:RX1
		'EAR PA Gain':0:POS_6_DB
		'DAC1 Switch':1:1
